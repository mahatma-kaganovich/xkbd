<global>

# font name, for an xft font, specify the point size after a '-'
# default font is fixed
#font verdana-12|fixed
font sans-16|fixed

# color of keboard background ( default white )
col  0:0:0

# color of an active key ( default black )
down_col  0:63:63

# color of key text ( default black )
txt_col 63:127:127

# color of key borders ( default black )
border_col 31:63:63

# color of non-chars
grey_col 0:31:31

# color of keypad
kp_col 0:31:31

# keyboard dimentions
#width  xxx
#height xxx

# button style, can be rounded ( default ), square or plain
# button_style plain
#repeat_delay 50
# 0 disable repeat (powersave), -1 - delay & time from X
repeat_time 0

</global>

#
# The keyboard definition is split into rows, starting from the top.
#
# options for each key are;
# 
#  default
#  shift
#  mod
#  default_ks
#  shift_ks
#  mod_ks
#  img
#  bg
#  fg
#  slide_up_ks
#  slide_down_ks
#  slide_left_ks
#  slide_right_ks
#  width
#  height

<layout>
<row>
	<key>
		default Esc
		default_ks Escape
#		width 4
	</key>
        <key>default_ks F1</key>
        <key>default_ks F2</key>
        <key>default_ks F3</key>
        <key>default_ks F4</key>
        <key>default_ks F5</key>
        <key>default_ks F6</key>
        <key>default_ks F7</key>
        <key>default_ks F8</key>
        <key>default_ks F9</key>
        <key>default_ks F10</key>
        <key>default_ks F11</key>
        <key>default_ks F12</key>
	<key>default_ks Print</key>
	<key>
		default SclLk
		default_ks Scroll_Lock
	</key>
	<key>default_ks Pause</key>
	<key>default_ks Home</key>
</row>
<row>
	<key>
		default_ks grave
		width 6
	</key>
	<key>default_ks 1</key>
	<key>default_ks 2</key>
	<key>default_ks 3</key>
	<key>default_ks 4</key>
	<key>default_ks 5</key>
	<key>default_ks 6</key>
	<key>default_ks 7</key>
	<key>default_ks 8</key>
	<key>default_ks 9</key>
	<key>default_ks 0</key>
	<key>default_ks minus</key>
	<key>default_ks equal</key>
	<key>
		default BkSp
		default_ks BackSpace
	</key>
	<key>
		default_ks Insert
	</key>
	<key>
		default_ks Delete
	</key>
	<key>
		default End
		default_ks End
		shift Begin
		shift_ks Begin
	</key>
</row>

<row>
	<key>
		shift LTab
		default_ks Tab
	</key>
	<key>default_ks q</key>
	<key>default_ks w</key>
	<key>default_ks e</key>
	<key>default_ks r</key>
	<key>default_ks t</key>
	<key>default_ks y</key>
	<key>default_ks u</key>
	<key>default_ks i</key>
	<key>default_ks o</key>
	<key>default_ks p</key>
	<key>default_ks bracketleft</key>
	<key>default_ks bracketright</key>
	<key>default_ks backslash</key>
</row>

<row>
	<key>
		default Caps
		default_ks Caps_Lock
		width 10
	</key>
	<key>default_ks a</key>
	<key>default_ks s</key>
	<key>default_ks d</key>
	<key>default_ks f</key>
	<key>default_ks g</key>
	<key>default_ks h</key>
	<key>default_ks j</key>
	<key>default_ks k</key>
	<key>default_ks l</key>
	<key>default_ks semicolon</key>
	<key>default_ks apostrophe</key>
	<key>
		default Enter
		default_ks Return
		width 11
	</key>
</row>

<row>
	<key>
		default Shift
		default_ks Shift_L
		width 14
	</key>
	<key>default_ks z</key>
	<key>default_ks x</key>
	<key>default_ks c</key>
	<key>default_ks v</key>
	<key>default_ks b</key>
	<key>default_ks n</key>
	<key>default_ks m</key>
	<key>default_ks comma</key>
	<key>default_ks period</key>
	<key>default_ks slash</key>
	<key>
		default ↑
		default_ks Up
		shift PgUp
		shift_ks Prior
		bg 0:31:31
	</key>
	<key>
		default Shift
		default_ks Shift_R
	</key>
</row>

<row>
	<key>
		default Ctrl
		default_ks Control_L
	</key>
	<key>
		default Super
		default_ks Super_L
		shift Hyper
		shift_ks Hyper_L
	</key>
	<key>
		default Alt
		default_ks Alt_L
	</key>
	<key>
		default i18l
		switch 0
	</key>
	<key>
		default_ks space
		width 21
	</key>
	<key>
		default Alt
		default_ks Alt_R
	</key>
	<key>
		default Super
		default_ks Super_R
		shift Hyper
		shift_ks Hyper_R
	</key>
	<key>
		default Menu
		default_ks Menu
	</key>
	<key>
		default Ctrl
		default_ks Control_R
	</key>
	<key>
		default ←
		default_ks Left
		bg 0:31:31
	</key>
	<key>
		default ↓
		default_ks Down
		shift PgDn
		shift_ks Next
		bg 0:31:31
	</key>
	<key>
		default →
		default_ks Right
		bg 0:31:31
	</key>
</row>
</layout>
